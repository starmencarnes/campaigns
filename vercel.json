{
  "functions": {
    "api/slack.js": {
      "runtime": "nodejs18.x",
      "memory": 512,
      "maxDuration": 10
    },
    "api/ideate.js": {
      "runtime": "nodejs18.x",
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/slack",
      "methods": ["POST"],
      "dest": "/api/slack.js"
    },
    {
      "src": "/api/ideate",
      "methods": ["POST"],
      "dest": "/api/ideate.js"
    }
  ]
}
